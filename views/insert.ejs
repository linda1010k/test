        <form class="needs-validation" action="/insert" method="GET" novalidate>
          <div class="col-md-3 mb-3">
            <label for="name">Name</label>
            <input
              type="text"
              name="name"
              id="name"
              placeholder="Linda"
              class="form-control"
              required
            />
            <div class="invalid-feedback">
              Please type in member's name.
            </div>
          </div>
          <div class="col-md-3 mb-3">
            <label for="age">Age</label>
            <input
              type="text"
              name="age"
              placeholder="18"
              class="form-control"
              required
            />
            <div class="invalid-feedback">
              Please type in member's age.
            </div>
          </div>
          <div class="col-md-3 mb-3">
            <label for="gender">Gender</label>
            <input
              type="text"
              name="gender"
              placeholder="Female"
              class="form-control"
              required
            />
            <div class="invalid-feedback">
              Please type in member's gender.
            </div>
          </div>
          <button
            class="w3-button w3-white w3-border w3-border-green w3-round-large"
            id="btn"
            type="submit"
            onclick="location.href"
          >
            Insert a member
          </button>
        </form>
        <footer class="footer">
          <p class="mt-5 mb-3 text-muted">Linda Lai &copy; 2020</p>
        </footer>
